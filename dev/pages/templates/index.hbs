	}

{{/config}}

{{> header}}

<div class="container">
	
	<div class="intro row">
		<div class="column grid-12">
			<div class="block">
				<div class="top-sep"></div>

				<div class="splash">
					<h1>Templates</h1>
					<h2>Free Templates By Sweefty</h2>
				</div>
			</div>
		</div>
	</div>

	<div class="section">
		<div class="row">
			<div class="column grid-12">
				<div class="block" style="text-align:left;">
					In our free time we create web templates, it's our hobby and we would like to share with you.
				</div>
			</div>
		</div>
	</div>

	<div class="section">
		<div class="row">
			
			<div class="column grid-6">
				<div class="block temp">
					<img alt="breeze template screenshot" src="../assets/images/templates/breeze.png">
					<div class="buttons">
						<a href="https://github.com/Sweefty/template-breeze/archive/master.zip" title="download breeze template" class="button small red">download</a>
						<a href="./breeze" target="_blank" title="breese template demo" class="button small blue">Demo</a>
						<a href="https://github.com/Sweefty/template-breeze" target="_blank" title="fork breeze template" class="button small green">Fork</a>
					</div>
				</div>
			</div>

			<div class="column grid-6">
				<div class="block temp">
					<img alt="bread template screenshot" src="../assets/images/templates/bread.png">
					
					<div class="buttons">
						<a href="https://github.com/Sweefty/template-bread/archive/master.zip" title="download bread template" class="button small red">download</a>
						<a href="./bread" target="_blank" title="view bread template demo" class="button small blue">Demo</a>
						<a href="https://github.com/Sweefty/template-bread" title="fork bread template on github" target="_blank" class="button small green">Fork</a>
					</div>
				</div>
			</div>
		</div>

		<div class="row">
			<div class="column grid-6">
				<div class="block temp">
					<img alt="breeze template screenshot" src="../assets/images/templates/framed.jpg">
					<div class="buttons">
						<a href="https://github.com/Sweefty/template-framed/archive/master.zip" title="download framed template" class="button small red">download</a>
						<a href="./framed" target="_blank" title="breese template demo" class="button small blue">Demo</a>
						<a href="https://github.com/Sweefty/template-framed" title="fork framed template" target="_blank" class="button small green">Fork</a>
					</div>
				</div>
			</div>
		</div>
	</div>

	<div class="section">
		<div class="row">
			
			<div class="column grid-6">
			<div class="block" style="text-align:center;margin-top:50px;height: 170px;">
        		<h1>Want More?</h1>
        		<p>Join our mailing list to be notified</a></p>
    		</div>
			</div>
			
			<div class="column grid-6">
				<div class="subscription block" style="margin-top:50px;height: 170px;">
					<!-- Begin MailChimp Signup Form -->
